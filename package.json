{
    "name": "fill_pg",
    "scripts": {
        "fillpg:run": "export UID=$(id -u) & docker-compose up -d",
        "fillpg:quit": "docker-compose down -v",
        "pg:run": "export UID=$(id -u) & docker-compose -f docker-compose-with-postgres.yaml up -d",
        "pg:quit": "docker-compose -f docker-compose-with-postgres.yaml down -v"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.ledgis.kr/lky5697/fill_pg.git"
    }
}